{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", 
  ".next/types/**/*.ts", "utils/motion.js", 
  "components/Animations/Parallax.jsx", "components/Contact/StarsCanvas.jsxree.jsx", "components/Contact/StarsCanvas.js", "components/Home/Tech/laptop-canvas.jsx", "components/Home/Tech/anim.jsx", "components/myModels/lights.jsx", "components/myModels/Earth/index-earth-2.jsx"],
  "exclude": ["node_modules"]
}
